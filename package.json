{
  "name": "laboratoire",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "pnpm -F docs dev",
    "dev:docs": "pnpm -F docs dev",
    "dev:react": "pnpm -F web-react dev",
    "dev:all": "concurrently -n DOCS,REACT -c green,cyan \"pnpm -F docs dev\" \"pnpm -F web-react dev\"",
    "build": "pnpm -F docs build && pnpm -F web-react build",
    "build:docs": "pnpm -F docs build",
    "build:react": "pnpm -F web-react build",
    "preview": "pnpm -F docs preview",
    "preview:docs": "pnpm -F docs preview",
    "preview:react": "pnpm -F web-react preview"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "concurrently": "^9.2.1",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.5"
  },
  "packageManager": "pnpm@10.0.0+sha512.b8fef5494bd3fe4cbd4edabd0745df2ee5be3e4b0b8b08fa643aa3e4c6702ccc0f00d68fa8a8c9858a735a0032485a44990ed2810526c875e416f001b17df12b"
}
